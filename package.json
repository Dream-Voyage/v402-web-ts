{
  "name": "@voyage_ai/v402-web-ts",
  "version": "0.1.5",
  "description": "v402pay platform frontend SDK for seamless Web3 payment integration with Solana and Ethereum support",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./react": {
      "types": "./dist/react/index.d.ts",
      "style": "./dist/react/styles.css",
      "import": "./dist/react/index.mjs",
      "require": "./dist/react/index.js"
    },
    "./react/styles.css": "./dist/react/styles.css"
  },
  "sideEffects": [
    "*.css",
    "dist/react/styles.css",
    "dist/react/index.js",
    "dist/react/index.mjs"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build:css": "tailwindcss -i ./src/react/styles.css -o ./dist/react/styles.css --minify",
    "build:js": "tsup",
    "build": "npm run build:js && npm run build:css",
    "build:prod": "BUILD_ENV=production npm run build:js && npm run build:css",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "updateLocal": "npm run build",
    "prepublishOnly": "npm run build:prod",
    "release": "npm run release:patch",
    "release:patch": "npm version patch && npm publish && git push --follow-tags",
    "release:minor": "npm version minor && npm publish && git push --follow-tags",
    "release:major": "npm version major && npm publish && git push --follow-tags"
  },
  "keywords": [
    "x402",
    "payment",
    "solana",
    "ethereum",
    "svm",
    "evm",
    "web3",
    "blockchain"
  ],
  "author": "",
  "license": "MIT",
  "peerDependencies": {
    "@ant-design/icons": "^5.6.1",
    "@solana/spl-token": "^0.4.0",
    "@solana/web3.js": "^1.95.0",
    "antd": "^5.29.1",
    "ethers": "^6.0.0",
    "react": ">=18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "antd": {
      "optional": true
    },
    "@ant-design/icons": {
      "optional": true
    }
  },
  "dependencies": {
    "x402": "^0.7.1",
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@ant-design/icons": "^5.6.1",
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "antd": "^5.29.1",
    "autoprefixer": "^10.4.22",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0"
  }
}
